local blacklistedKeywords = {}
local blacklistedDates = {}

local function addItems(TABLE, items)
	for _, item in ipairs(items) do
		table.insert(TABLE, item)
	end
end

local function removeItems(TABLE, items)
	for _, item in ipairs(items) do
		for i = #TABLE, 1, -1 do
			if TABLE[i] == item then
				table.remove(TABLE, i)
			end
		end
	end
end

local function setTheme()
	local themeColor
	for _, v in pairs(gamesPageFrame.Sort:GetDescendants()) do
		if v.Name == "ThemeButtonGradient" then
			themeColor = v.Color
			return
		end
	end
	
	if themeColor == nil then
		warn("Theme Not found. Sticking with the default!")
	end

	for _, v in pairs(Categories:GetDescendants()) do
		if v.Name == "ThemeButtonGradient" then
			v.Color = themeColor
		end
	end
end


local function filterGames()
	local gamesList = gamesPageFrame.List
	for _, v in pairs(gamesList:GetChildren()) do
		if v:IsA("TextButton") then
			local title = string.lower(v.BottomPart.NameLabel.Text)
			local dateString = v.HoverExpansion.TimeLabel.Text
			for _, word in pairs(blacklistedKeywords) do
				local lowerCaseWord = string.lower(word)
				if string.find(title, lowerCaseWord) then
					v.Visible = false
				end
			end
			for _, date in pairs(blacklistedDates) do
				if string.find(dateString, date) then
					v.Visible = false
				end
			end
		end
	end
end

local function unfilterGames()
	local gamesList = gamesPageFrame.List
	for _, v in pairs(gamesList:GetChildren()) do
		if v:IsA("TextButton") then
			if v.Visible == false then
				local title = string.lower(v.BottomPart.NameLabel.Text)
				local dateString = v.HoverExpansion.TimeLabel.Text
				for _, word in pairs(blacklistedKeywords) do
					local lowerCaseWord = string.lower(word)
					
					if string.find(title, lowerCaseWord) then return end
					
					v.Visible = true
				end
				for _, date in pairs(blacklistedDates) do
					
					if string.find(dateString, date) then return end
					
					v.Visible = true
				end
			end
		end
	end
end

local function changedCheckBox(box)
	local boxName = box.Parent.Name
	if box.CheckImage.Image == "rbxassetid://5226924982" then -- TICK Icon
		box.CheckImage.Image = "rbxassetid://10152135063"
		-- DATES
		if boxName == "2015" then
			addItems(blacklistedDates, {"Mid 2015", "Late 2015", "Early 2015"})
		elseif boxName == "2014-2013" then
			addItems(blacklistedDates, {"Mid 2014", "Late 2014", "Early 2014", "Mid 2013", "Late 2013", "Early 2013"})
		elseif boxName == "2012-2011" then
			addItems(blacklistedDates, {"Mid 2012", "Late 2012", "Early 2012", "Mid 2011", "Late 2011", "Early 2011"})
		elseif boxName == "2010-2009" then
			addItems(blacklistedDates, {"Late 2009", "Late 2010", "Early 2009"})

			-- KEYWORDS
		elseif boxName == "Hangout" then
			addItems(blacklistedKeywords, {"hangout", "chill", "tv", "movie"})
		elseif boxName == "Owner" then
			addItems(blacklistedKeywords, {"owner", "online"})
		elseif boxName == "Admin" then
			addItems(blacklistedKeywords, {"admin", "free", "mod"})
		elseif boxName == "Simon" then
			addItems(blacklistedKeywords, {"simon", "says"})
		else
			return
		end

		filterGames()
		print(blacklistedDates)
		print(blacklistedKeywords)
	else
		box.CheckImage.Image = "rbxassetid://5226924982"

		-- DATES
		if boxName == "2015" then
			removeItems(blacklistedDates, {"Mid 2015", "Late 2015", "Early 2015"})
		elseif boxName == "2014-2013" then
			removeItems(blacklistedDates, {"Mid 2014", "Late 2014", "Early 2014", "Mid 2013", "Late 2013", "Early 2013"})
		elseif boxName == "2012-2011" then
			removeItems(blacklistedDates, {"Mid 2012", "Late 2012", "Early 2012", "Mid 2011", "Late 2011", "Early 2011"})
		elseif boxName == "2010-2009" then
			removeItems(blacklistedDates, {"Late 2009", "Late 2010", "Early 2009"})

			-- KEYWORDS
		elseif boxName == "Hangout" then
			removeItems(blacklistedKeywords, {"hangout", "chill", "tv", "movie"})
		elseif boxName == "Owner" then
			removeItems(blacklistedKeywords, {"owner", "online"})
		elseif boxName == "Admin" then
			removeItems(blacklistedKeywords, {"admin", "free", "mod"})
		elseif boxName == "Simon" then
			removeItems(blacklistedKeywords, {"simon", "says"})
		else
			return
		end
		
		unfilterGames()
		print(blacklistedDates)
		print(blacklistedKeywords)
	end
end

gamesPageFrame.List.ChildAdded:Connect(function(child)
	if child:IsA("TextBox") then
		filterGames()
	end
end)

for _, v in pairs(List:GetDescendants()) do
	if v:IsA("TextButton") then
		if v.Name == "Check" then
			v.MouseButton1Click:Connect(function()
				changedCheckBox(v)
			end)
		end
	end
end

setTheme()
